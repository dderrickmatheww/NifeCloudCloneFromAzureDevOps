    "axios": "^0.26.1",
    "expo-server-sdk": "^3.6.0",
    "firebase-admin": "^9.9.0",
    "firebase-functions": "^3.14.1",
    "geolib": "^3.3.1",
    "nodemailer": "^6.7.3",
    "us-state-converter": "^1.0.5",
    "xhr2": "^0.2.0"
  },
  "devDependencies": {
    "@google-cloud/functions-framework": "^3.1.0",
    "eslint": "^5.12.0",
    "eslint-plugin-promise": "^4.0.1",
    "firebase-functions-test": "^0.2.0",
    "install": "^0.13.0",
    "prisma": "^3.12.0"
  },
